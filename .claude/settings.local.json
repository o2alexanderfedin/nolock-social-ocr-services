{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(dotnet new:*)",
      "Bash(dotnet sln add:*)",
      "Bash(dotnet add reference:*)",
      "Bash(rm:*)",
      "Bash(dotnet test)",
      "Bash(dotnet test:*)",
      "Bash(dotnet build:*)",
      "Bash(gh repo create:*)",
      "Bash(git init:*)",
      "Bash(git remote add:*)",
      "Bash(git flow init:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git flow feature start:*)",
      "Bash(git push:*)",
      "Bash(git flow feature finish:*)",
      "Bash(git flow release:*)",
      "Bash(GIT_MERGE_AUTOEDIT=no git flow release finish -n 1.0.0)",
      "Bash(dotnet add package:*)"
    ],
    "deny": []
  }
}